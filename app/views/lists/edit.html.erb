<h1>edit</h1>

<% if @list.errors.any? %>
  <%= @list.errors.count %>件のエラー
  <ul>
    <% @list.errors.full_messages.each do |message| %>
      <li><%= message %></li>
    <% end %>
  </ul>
<% end %>

<%=form_with model: @list,url: update_list_path(@list.id),method: :patch,local:true do|f|%>

<%=f.label:title%><br>
<%=f.text_field:title%><br>
<%=f.label:body%><br>
<%=f.text_area:body%><br>
<%=f.label:image%><br>
<%=f.file_field:image%><br>
<%=f.submit'complate'%>
<%end%>